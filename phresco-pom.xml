<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0">
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.photon.phresco</groupId>
    <artifactId>Drupal7Eshop</artifactId>
    <packaging>phresco</packaging>
    <name>Drupal7Eshop</name>
    <version>1.0.0-SNAPSHOT</version>
    <scm>
        <connection>https://github.com/sakthi-ra/Drupal7Eshop_phresco.git</connection>
        <developerConnection>scm:git:https://github.com/sakthi-ra/Drupal7Eshop_phresco.git</developerConnection>
        <url>scm:git:https://github.com/sakthi-ra/Drupal7Eshop_phresco.git</url>
    </scm>
    <build>
        <plugins>
            <plugin>
                <groupId>com.photon.phresco.plugins</groupId>
                <artifactId>phresco-maven-plugin</artifactId>
                <version>3.2.0.13001</version>
                <extensions>true</extensions>
            </plugin>
        </plugins>
    </build>
   <properties>
        <phresco.src.root.dir xmlns="">../Drupal7Eshop/</phresco.src.root.dir>
        <phresco.src.repo.url xmlns="">https://github.com/sakthi-ra/Drupal7Eshop_src.git</phresco.src.repo.url>
        <phresco.split.src.dir xmlns="">Drupal7Eshop</phresco.split.src.dir>
        <phresco.split.phresco.dir xmlns="">Drupal7Eshop-phresco</phresco.split.phresco.dir>
        <phresco.dotphresco.repo.url xmlns="">https://github.com/sakthi-ra/Drupal7Eshop_phresco.git</phresco.dotphresco.repo.url>
        <phresco.split.test.dir xmlns="">Drupal7Eshop-test</phresco.split.test.dir>
        <phresco.test.repo.url xmlns="">https://github.com/sakthi-ra/Drupal7Eshop_test.git</phresco.test.repo.url>
        <source.pom>pom.xml</source.pom>
    </properties>
</project>
